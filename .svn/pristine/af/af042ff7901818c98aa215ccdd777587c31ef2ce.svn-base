.jcarousel-prev, .jcarousel-next {
  position: absolute;
  top: 40%;
  left: 0;
  width: 40px;
  height: 40px;
  margin-top: -20px;
  font-family:Times New Roman;
  font-size: 60px;
  font-weight: 100;
  line-height: 30px;
  color: #0072cc;
  text-align: center;
  background: transparent;
  border: 1px solid #a8a8a8;
  -webkit-border-radius: 23px;
  -moz-border-radius: 23px;
  border-radius: 23px;
  opacity: 1;
  filter: alpha(opacity=100);
  text-decoration:none;
}
.jcarousel-next {
  right: 0;
  left: auto;
}
