/*products*/
.products_name{ width:745px !important; height:30px; padding-top:5px; padding-left:15px; background:#00a1e6; color:#ff0; font:bold 10px/26px Arial; border-radius: 30px 30px 30px 30px;}

.products{width:100%; display:block; overflow:hidden;margin-bottom: 5px;}
.products ul{ display:block; margin-top:10px; overflow:hidden;/* margin-bottom:-30px !important;*/}
.products ul li{float:left; width:169px; border-right:1px dotted #ccc; position:relative; padding:0 10px 10px; margin:0 0 10px; background:white}
.products ul li span{font:normal 11px/18px Arial; color:#444; background:url(images/giare/arow-list.jpg) no-repeat left center; 
display:block; padding-left:10px;}

.products ul li img{margin:20px auto; display:block; max-width:120px; height:auto}
.products ul li a{font:normal 14px/18px Arial; color:#0072c1; text-decoration:none; text-align:center;}
.products ul li a.title{overflow: hidden;text-overflow: ellipsis;white-space: nowrap;}
.products ul li a:hover{text-decoration:underline}

.priceold{font:normal 14px/20px Arial; color:#333; text-decoration:line-through; display:block; text-align:center;margin:2px}
.priceoldnoshow{font:normal 14px/20px Arial; color:#333; display:block; text-align:center;margin:2px}
.pricenew{font:bold 17px/25px Arial; color:#da0300; display:block; text-align:center; margin:2px}
.special{position:absolute; top:-20px; right:0; width:35px; height:35px; margin:0 !important}

.buy-now {
    display:block;
    font:bold 14px Arial !important;
    text-transform:uppercase;
    text-shadow:1px 1px 0 #006dc5 !important;
    padding: .4em 0.6em ;
	color: #fff !important;
	border:1px solid #0090cd;
	border-radius:4px;
	box-shadow: 0 1px 0 rgba(255, 255, 255, 0.1) inset, 0 1px 2px rgba(0, 0, 0, 0.5);
	background-color: #00a1e5;
    background-image: -moz-linear-gradient(top, #00a1e5, #0086be);
    background-image: -ms-linear-gradient(top, #00a1e5, #0086be);
    background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#00a1e5), to(##0086be));
    background-image: -webkit-linear-gradient(top, #00a1e5, #0086be);
    background-image: -o-linear-gradient(top, #00a1e5, #0086be);
    background-image: linear-gradient(top, #00a1e5, #0086be);
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#00a1e5', endColorstr='#0086be', GradientType=0);
    filter: progid:dximagetransform.microsoft.gradient(enabled=false);
}
.buy-now:hover {background: #079eed; text-decoration:none !important; color:#ff0 !important}

.products ul li:hover .Kmai{display:block}
.Kmai{
	display:none;
	position:absolute;
	top:45%; left:0;
	font:normal 14px Arial;
	padding:6px ;
	color: #ff0;
	background:#00a1e6;
	width:178px;
}


