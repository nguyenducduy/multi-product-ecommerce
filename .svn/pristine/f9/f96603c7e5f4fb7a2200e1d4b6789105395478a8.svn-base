/*pop-up*/

#wrap_game1900{width:980px; margin:auto; overflow:hidden}
.banner{position:relative;height: 180px;overflow: hidden;}
.banner #leftbanner{float: left;width: 648px;}
.banner #rightbanner{float: right;width: 330px;background: #00A1E6;color: #fff;height: 180px;margin-left: 2px;}
.banner #rightbanner h4{clear: both; font-size: 20px; text-align: center;font-weight: 700;margin-top: 5px;padding:0px;}
.banner #rightbanner p{clear: both; font-size: 14px; padding:5px 0;margin: 0px;}
.banner #rightbanner p strong{color: #ff0;font-size: 20px;}
.logo{position:absolute; top:0; left:0}


#game_content{width:100%; overflow:hidden; margin:20px auto}

#general{float:left; width:200px; overflow:hidden}

#general_nav{display:block; list-style:none; margin:0; /*border-radius:10px;*/ overflow:hidden}
#general_nav li{float:none; display:block; line-height:20px; position:relative}
#general_nav li a{display:block; padding:18px 8px; font:normal 13px Arial; color:#00a1e6; text-align:left; text-decoration:none; /*border-bottom: 1px solid #fff;*/
 background:#fafafa; text-transform:uppercase;}

#general_nav li a:hover{color:#00a1e6; background:#dce9e6;}

.icon_1{background:#ff0;border-radius: 35px 35px 35px 35px;color:#00a1e6; font: bold 20px Times New Roman; height: 30px; padding-top: 5px;position: absolute;
    left: 5px;text-align: center;top: 8px;width: 35px;
}

.icon_2{position:absolute; top:8px; left:15px; width:35px; height:30px; padding-top:5px; border-radius:35px 35px 35px 35px; text-align:center;
font: bold 20px Times New Roman; color:#fff; background:#00a1e6;
}


#banner_other{width:200px; margin:10px auto;}

#game_right{float:right; width:760px; overflow:hidden}

.form-info{width:100%; display:block; overflow:hidden; }
.formfield{width:740px; padding:10px; margin-bottom:12px; overflow:hidden; font:normal 14px/18px Arial; color:#333;background:#FAFAFA; border-radius: 10px 10px 10px 10px;}
.formtitle{font:normal 16px/18px Arial; color:#ff0; text-align:center; float:none; clear:both; display:block; margin-bottom:6px; padding-top:6px; height:25px; background:#00a1e6; border-radius:4px;}
.formtitle2{font:normal 14px/18px Arial; color:#00a1e6; text-align:left; float:left; clear:both; display:block; margin-top:2px; width:360px;}

.propnput{outline:none;width:240px!important; /*line-height:20px; */padding:5px 8px!important; margin:0 0 3px!important ; border-radius:4px; border:1px solid #ccc; color:#777; font:normal 12px Arial; margin-top: 7px !important}
.propnput:focus{border:1px solid #00a1e6}

.propnput2{outline:none;width:95px; line-height:20px; padding:5px 8px;/* margin:8px 0;*/ border-radius:4px; border:1px solid #ccc; color:#777; font:normal 12px Arial; margin-top: 12px !important}
.propnput2:focus{border:1px solid #00a1e6}

.note{font:normal 11px/16px Arial; color:#888; font-style:italic; margin-top:3px;}
.notered{color: red !important; float: left !important; font-size: 12px !important; margin-right: 8px; margin-bottom:0 !important; text-align: left !important;}

.btngame{ outline:none;cursor:pointer;padding:6px 8px; margin-top:5px; text-align:center; float:right; background:#00a1e8; font:bold 14px Arial; color:#ff0; border-radius:4px 4px 4px 4px; border:none}
.btngame:hover{background:#09C; color:#fff}

.infouser{width:100%; margin-bottom:12px; overflow:hidden; font:normal 14px/18px Arial; color:#333; background:white; border-radius:10px 10px 10px 10px}

.infouser_1{float:left;/* width:370px;*/height:45px; margin:10px 5px 0; font-size:13px;line-height:20px;text-align:center;}
.infouser_1 span{color:#00a1e6;}

.infouser_2{padding:4px 10px;line-height:20px;float: right;}
.infouser_2 img{margin-right: 2px;border: none;}
.infouser_2 a, span{margin-left: 10px;margin-top:7px;display: inline-block;text-decoration: none;color:#fff;}
