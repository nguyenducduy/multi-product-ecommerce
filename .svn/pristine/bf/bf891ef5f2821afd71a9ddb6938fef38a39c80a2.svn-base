#profiledropdown .caret{border-top-color:#fff; margin-left:5px;}
#navbar{width:178px; position: fixed; top: 40px; z-index:220; bottom:0; background:#f2f7fa; box-shadow:-1px 0 0 #DDE2EB inset; overflow:hidden; box-sizing: border-box; --moz-box-sizing: border-box; --webkit-box-sizing: border-box;}
