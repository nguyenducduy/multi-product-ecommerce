

/* customize from bootstrap */
body {padding-top: 0px; font-size: 12px;}


.sidebar-nav {padding: 0;}
h1,h2,h3{line-height:1; margin:0;}
h1{font-size:1.5em;}
h2{font-size:1.2em;}
h3{font-size:1.1em; padding-bottom:10px;}
footer{font-size:11px; color:#ccc; text-align:right;}
.clear, .cl{clear:both;}

a{color:#3498db;}
a:hover{color:#e67e22; text-decoration:none;}
.hideinit{display:none;}
.red{color:#f00;}




.page-header{padding-bottom:4px; margin:0 0 10px 0;}
.page-header h1{margin-top:0; text-transform:uppercase; text-shadow: 1px 1px 0 #fff;}
#topbar .logo{color:#fff; line-height:40px; font-weight:bold; font-size:16px; padding:14px 16px;margin-left:-10px;}
.navbar-inner{background: #3498db; height:40px;}
.navbar-fixed-top .navbar-inner, .navbar-static-top .navbar-inner{box-shadow:none;-webkit-box-shadow:none;border-width:0;}

.form-search .input-append .search-query{border-radius: 4px 0 0 4px; -webkit-border-radius: 4px 0 0 4px;border-color:#fff;}
.form-search .input-append .btn{border-radius: 0 4px 4px 0; -webkit-border-radius: 0 4px 4px 0; border-width:0; text-shadow:none; -webkit-text-shadow:none; font-weight:normal;}
.form-search .input-append .btn:hover{background:#ddd;}
.tab-content .form-inline{padding: 10px !important;}

select, textarea, input[type="text"], input[type="password"], input[type="datetime"], input[type="datetime-local"], input[type="date"], input[type="month"], input[type="time"], input[type="week"], input[type="number"], input[type="email"], input[type="url"], input[type="search"], input[type="tel"], input[type="color"], .uneditable-input
{
	font-size:12px;
	border-radius: 4px;
	-webkit-border-radius: 4px;
}

/* flat */
