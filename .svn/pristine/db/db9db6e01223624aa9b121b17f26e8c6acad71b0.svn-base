.installment{position:relative; margin:10px auto; width:900px;  padding:2px 2px 15px; overflow:hidden; border-radius:6px; font:normal 13px/20px Arial; color:#333;background: #fff;}

.installment p{ position:relative; padding:8px; border-bottom:1px solid #ccc; font:bold 14px Arial; color:#444; text-transform:uppercase}
.installeft{float:left; width:520px; margin-right:30px; overflow:hidden; font-size:13px; margin:8px}
.installeft span{font:bold 14px/20px Arial; color:#333; text-transform:uppercase; display:block;}
.installment label{display:inline-block; width:115px; margin-left:0;text-align: right; margin-right:8px; font-size:13px;}
.wi-ins01{width:85px; display:inline; margin-right:10px;}
.wi-ins02{width:255px !important;/*width:323px !important;*/}
.wi-ins03{width:115px !important;/*width:165px !important;*/}
.wi-ins04{margin-left:127px; margin-right:10px; line-height:20px;}
.wi-ins05{width:356px !important; margin-top:10px}
.wi-ins06{width:130px !important;}
.wi-ins07{width:125px !important; margin:10px auto 10px 215px; padding:8px}
.wi-ins08{margin-left:140px; margin-right:10px; line-height:20px;}

.installright{float:left; width:350px; overflow:hidden; margin-top:10px;}

.installrightsp{overflow:hidden; clear:both}
.installrightsp img{display:inline; vertical-align:text-top; float:left; max-width:200px; max-height:200px}
.installrightsp span{font:bold 13px/20px Arial; color:#00a1e4}
.installrightsp .priceins{font:bold 14px Arial; color:red; }

.infoonemonth{margin-top:10px; overflow:hidden}
.infoonemonth span{font:bold 14px/20px Arial; color:#333; text-transform:uppercase; display:block;}
.infoonemonth a{font:normal 13px/20px Arial !important; color:#0082cc; text-decoration:none}
.infoonemonth a:hover{text-decoration:underline}

.infopay{overflow:hidden; background:#FFFDD4; margin:10px 0; padding:10px;}
.infopay .do{font:bold 13px/20px Arial; color:red; display:inline; text-transform:none}
