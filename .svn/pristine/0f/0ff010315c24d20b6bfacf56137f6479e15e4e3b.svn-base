/* FACEBOOK SHARE */
#fbhead{background:#6d81b8 url(../images/facebook_icon.png) no-repeat left center; background-position-x:5px; color:#fff; font-size:14px; font-weight:normal;padding:5px 5px 5px 25px;}
#fbtitle{padding:10px 10px 0 10px;font-weight:bold;color:#444;font-size:larger;}
#fbsharewrapper{margin:10px; border:1px solid #ccc;}
#fbsharewrapper textarea{padding:10px;font-size:13px; line-height:18px;width:100%;height:120px; border:0px solid #eee; box-sizing:border-box;-moz-box-sizing:border-box;-webkit-box-sizing:border-box;}
#fblinkcontainer{margin-top:10px; border-top:1px dashed #ccc; padding:10px;}
#fblinkimage{width:80px; float:left;}
#fblinkimage img{width:70px;}
#fblinkmeta{margin-left:80px;}
#fblinkname{color:#444; font-size:larger; font-weight:bold;}
#fblinkcaption{color:#666;padding:2px 0 5px 0;}
#fblinkdescription{line-height:1.5; color:#222;}
#fbsharesubmit{background:#ccc;padding:5px;height:22px;}
#fbsharebutton{float:right;height:22px;line-height:22px;background:#6077b0; letter-spacing:1px; color:#fff; text-decoration:none;border:1px solid #294083;}
#fbsharebutton span{border-top:1px solid #8a9ac5;display:block;padding:0 20px;}
#fbsharebutton:hover{background:#3a569b;}
#fbsharesubmit .tmp_indicator{float:right;}
#fbprivacy{float:right; padding:5px 10px 5px 17px;background:url(../images/facebookshare-privacy.png) no-repeat left;}
