body{margin:0; padding:0;position:relative;font-family: "Segoe UI", Tahoma, Geneva, sans-serif; font-size:11px; background:#fff; overflow-y:scroll;}
p, h1, form, input{border:0; margin:0; padding:0;}
.spacer{clear:both; height:1px;}
.hide{display:none;}
.cl{clear:both;}
.fr{float:right;}
.fl{float:left;}
.clear{clear:both; height:0px;}
a{color:#06f; text-decoration:none;}
a:hover{color:#F60; text-decoration:underline;}
a img{border:0;}
.red{color:#f00;}
.greenfree{background-color:#b0d46f !important;}
input.textbox{padding:2px 5px;}
input.button{padding:2px 10px; background:#ccc;}

#wrapper{width:960px; margin:auto; background:#fff url(../images/openbook_body.png) right repeat-y;}

#maintain{display:none;}

/* header for normal controllergroup */
#mybooklistmenu{height:30px; float:right; width:700px; border-bottom:1px solid #eee; margin:10px 10px 10px 0; background:#fff ;}
#mybooklistmenu a{color:#000; font-size:12px; font-weight:normal;}
#mybooklistmenu a:hover{color:#fe4a63;}
#mybooklistmenu a.current{font-weight:bold; color:#000; cursor:default;}
#mybooklistmenu a span{text-decoration:underline; color:#00f; cursor:pointer; font-weight:normal;}
#mybooklistmenu .left{float:left; font-size:12px; line-height:30px; padding:3px 0 0 0px; color:#555;}
#mybooklistmenu .left a{float:left;padding-top:4px; border:0; line-height:normal; padding-right:5px;}
#mybooklistmenu .left #fsitebooksearchtext{width:150px; font-size:12px; border:1px solid #cacaca; padding:2px}
#mybooklistmenu .left #fsitebooksearchsubmit{margin-left:2px; padding:2px 10px; font-size:12px; background:#fff; color:#000; cursor:pointer;}
#mybooklistmenu .right{float:right; padding-top:1px; padding-right:0px;}
#mybooklistmenu .right .text{line-height:27px; float:right; padding:0 2px 0 3px;}
#mybooklistmenu .right img{float:right;border:0; margin:5px 0 0 8px;}
#mybooklistmenu .right img.icon{width:16px;}

/* header for user controller group, with dark blue background top bar */
#header{ width:100%; height:30px; background:#005274; margin-bottom:0px; position:fixed;box-shadow:0 1px 0px #111;-webkit-box-shadow:0 1px 0px #111;z-index:10000;}
#header .left{float:left;width:244px;}
#header .left img{border:0;}
#header h1{float:left;}
#header h1 a{display:block; width:110px; height:30px; }

/* TOP NOTIFICATION */
#topnotify{float:right;}
#topnotify .topitem{float:left;margin-left:5px;}
#topnotify .topbutton{padding:5px 4px;display:block; width:18px; height:18px; margin-top:3px; position: relative;}
#topnotify .topbutton .sp{width:18px; height:18px; display:block;opacity:0.3;}
#topnotify .topbutton:hover .sp{opacity:1;}

#topnotify .topbuttonlocation{padding:5px 4px;display:block; width:18px; height:18px; margin-top:3px; position: relative;}
#topnotify .topbuttonlocation .sp{width:18px; height:18px; display:block;opacity:0.3;}
#topnotify .topbuttonlocation:hover .sp{opacity:1;}


#topnotify .badge{position:absolute; font-family: Arial, sans-serif; color:#fff; background:#f00; padding:0px 2px; border-radius:2px; -webkit-border-radius:2px; font-size:10px; margin:-22px 0 0 12px;}
#topnotify .active{background:#fff; border-radius: 2px 2px 0 0; -webkit-border-radius: 2px 2px 0 0;}
#topnotify .active .sp{opacity:1;}
#topnotify .active .badge{display:none;}
#topnotify .topitempanel{position:absolute; width:310px;background: #fff;border: 3px solid #666;border-top-width: 0;margin-left: -3px;border-radius: 0 0 4px 4px;box-shadow: 0px 2px 3px #888; -webkit-box-shadow:0px 2px 3px #888;}
#topnotify .topitempanel h3{padding:10px; margin:0; font-size:11px; border-bottom:1px solid #eee}
#topnotify .topitempanel h3 a{font-size:11px; font-weight:normal; float:right;}
#topnotify .topitempanel .viewall{clear:both;height:30px;}
#topnotify .topitempanel .viewall a{display:block; height:30px; line-height:30px; text-align:center; background:#eee; color:#000;}
#topnotify .topitempanel .viewall a:hover{background:#666; color:#fff; text-decoration: none;}
#topnotify .topitempanel ul{list-style-type:none; margin:0;padding:0; max-height:300px; overflow:auto; }
#topnotify .topitempanel li{border-top:1px solid #eee; clear:both;}
#topnotify .topitempanel li:first-child{border-top-width:0;}
#topnotify .topitempanel li:hover{background:#f5f5f5; text-decoration:none;}
#topnotify .topitempanel li img.avatar{width:50px; height:50px; float:left; padding:10px;}
#topnotify .topitempanel li .info{float:left; width:220px;line-height:1.5; color:#555;}
#topnotify .topitempanel li .info a{color:#000; display:block; padding:10px 0 10px 0px;}
#topnotify .topitempanel li .info a:hover{text-decoration:none;}
#topnotify .topitempanel li.friendrequestitem .info{width:150px;}
#topnotify .topitempanel li .info a.requestitem{font-weight:bold; color:#06f; padding-bottom:3px;}
#topnotify .topitempanel li .request{float:right; padding:2px 8px; border-radius:8px; -webkit-border-radius:8px; float:right; margin:10px 5px 0 0; background:#eee; color:#999;}
#topnotify .topitempanel li .request:hover{background:#000; text-decoration:none; color:#fff;}
#topnotify .topitempanel li.friendrequestitem .tmp_indicator{float:right; margin:10px;}


