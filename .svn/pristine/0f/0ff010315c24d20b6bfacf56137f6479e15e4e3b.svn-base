/*	PAGINATION	*/
.pagination {text-align: right;padding: 20px 0 5px 0;margin-bottom:20px; font-size: 12px; line-height:2.3}
.pagination a { margin: 0 5px 0 0;padding: 5px 10px;color:#000;font-style:normal;font-size:12px; border-radius:4px; -webkit-border-radius:4px;}			
.pagination a:hover {color:#F06;background:#eee; text-decoration:none;}
.pagination a.number {border: 1px solid #ddd;}
.pagination a.current {background: #444444 !important;border-color: #333 !important;color: #fff !important;}		
.pagination a.numbernext{background:#f90; font-weight:bold; color:#fff;padding:6px 15px;border:2px solid #fff;}
.pagination a.numbernext:hover{background:#09f;}
.pagination a.current:hover {text-decoration: none;}


