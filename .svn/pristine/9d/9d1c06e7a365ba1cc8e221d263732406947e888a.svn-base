/*Search result*/
.wrsearch {font:13px Arial; color:#333; overflow:hidden;display: block; margin: 30px auto;width:1200px;}
.search-ct{float:right; width:960px; overflow:hidden}
.search-ct h3{font:20px/25px arial;color:#666; display:block;padding:0 8px 8px;}
.search-left{width:210px;float:left;margin-right:10px;}
.search-left ul{list-style-type:none;margin-bottom:20px; display:block; overflow:hidden}
.search-left ul li.bar-blue{background:#00a1e5;	color:#fff;	font:bold 13px/19px arial;padding:9px 0;text-align:center;}
.search-left ul li .bullet{color:#444;float:left;font:bold 12px arial;margin:0 7px;}
.search-left ul li .icon-del{color:#fff;float:left;	background:red;	width:8px;height:14px;padding: 2px 3px 1px 6px;border-radius:10px;	font:bold 12px arial;margin:0 7px;}
.search-left ul li a{display:block;	padding:11px 0;	color:#3c6077;font-size:13px;border-top: solid 1px #ececec;text-decoration:none;}
.search-left ul li.current a{color:#00a1e5;}
.search-left ul li a:hover{text-decoration:none;color:#00a1e5;}
/*pagination*/
.paginat{overflow:hidden; display:block; margin:20px 0; float:right}
.paginat ul{display:block; margin:0; padding:0; list-style:none}
.paginat li{float:left; line-height:20px; margin-left:5px}
.paginat li a{display:block; border:1px solid #ccc; background:#fff; border-radius:4px; padding: 5px 10px; font:13px Arial; color:#333; text-decoration:none}
.paginat li a:hover{background:#f0f0f0}
.paginat li a.actived{border:1px solid #00a1e6; color:#ff8400}
.paginat li a.prevpagi{color:#00a1e6}
.paginat li a.nextpagi{color:#00a1e6}

/*Back to top button */
#back-top {position: fixed; bottom: 30px;right:30px; z-index:999}
#back-top a {outline:none;display: block;text-align: center;font: 11px/100% Arial, Helvetica, sans-serif;text-transform: uppercase; text-decoration: none;color: #bbb;-webkit-transition: 1s; -moz-transition: 1s;transition: 1s;}
#back-top a:hover {color: #000;}
#back-top span {width:60px;height:60px;display: block;margin-bottom: 7px; margin-left:15px;background: #ff8400  url(/templates/default/images/site/up-arrow.png) no-repeat center center; -webkit-border-radius: 8px; -moz-border-radius: 8px;border-radius: 8px; -webkit-transition: 1s; -moz-transition: 1s;transition: 1s;}
#back-top a:hover span {background-color: #777;}
#back-top strong{font-weight:normal; background:white !important; padding:5px}
