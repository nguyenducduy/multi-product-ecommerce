.newsbotinfo ul{margin:0; padding:0; list-style:none; overflow:hidden}
.newsbotinfo li{float:none; line-height:20px; display:block; overflow:hidden; padding:2px 0}
.newsbotinfo li a{font:13px Arial; color:#333; padding:8px 5px; text-decoration:none; padding:0 5px}
.newsbotinfo li a:hover{text-decoration:underline}

.newsbotsocial{float:left; overflow:hidden; width:280px;}
.newsbotsocial span{overflow:hidden; display:block; font:15px Arial; color:#00a1e6; display:block; margin:10px 0; padding:5px 0; text-transform:uppercase}
.newsbotsocial ul{margin:0; padding:0; list-style:none; overflow:hidden}
.newsbotsocial li{float:none; line-height:20px; display:block; overflow:hidden}
.newsbotsocial li a{font:13px Arial; color:#333; padding:8px 5px; text-decoration:none}
.newsbotsocial li a:hover{text-decoration:underline}

/*Responsive*/
@media (min-width: 1024px) and (max-width: 1200px) {
	.newsmid{width:610px;}
	.swiper-content .movie-pic{width:325px;}
	.classify{width:610px;}
	.classify a{display:block; float:none}
	.classifylist{display:block; margin-left:0; border-left:0; width:100%}
	.classifylist ul li{float:left; width:280px; padding:5px 0; margin-right:10px; border-top:1px dotted #e5e5e5}
	.classifylist ul li a{font-size:12px;}
	.classify div a{float:left}
	.newsbotcopy{width:320px; margin-left:10px;}
	.newsbotinfo{width:200px;}
	.newsbotsocial{width:200px;}
	.newsbortop{width:600px}
	.newsresult{width:480px; height:165px}
}

@media screen and (max-width: 1024px) {	
	.newsmid{width:610px;}
	.swiper-content .movie-pic{width:325px;}
	.classify{width:610px;}
	.classify a{display:block; float:none}
	.classifylist{display:block; margin-left:0; border-left:0; width:100%}
	.classifylist ul li{float:left; width:280px; padding:5px 0; margin-right:10px; border-top:1px dotted #e5e5e5}
	.classifylist ul li a{font-size:12px;}
	.classify div a{float:left}
	.newsbotcopy{width:320px; margin-left:10px;}
	.newsbotinfo{width:200px;}
	.newsbotsocial{width:200px;}
	.newsbortop{width:600px}
	.newsresult{width:480px; height:165px}
	.searchhd{width:520px;}
}