.swiper-nav .swiper-slide {padding:8px 5px;width:240px;height:50px; color:white; cursor:pointer;position: relative;border-top:3px solid white; border-right:1px solid white}

.swiper-nav .swiper-slide, .swiper-nav .angle {
	-webkit-transition: 300ms;	-moz-transition: 300ms;	-ms-transition: 300ms;	-o-transition: 300ms;transition: 300ms;}
.swiper-nav .angle {
	width: 0;height: 0;	position: absolute;	left: 50%;margin-left: -3px;top: -1px;opacity: 0;
	border-left: 8px solid transparent;
	border-right: 8px solid transparent;
	border-top: 8px solid #fbdf02;
}
