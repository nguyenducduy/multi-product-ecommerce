/*========================*/
.m-accordion {color: #000}
.m-accordion .m-header {
height:40px;line-height: 40px;	padding: 0 5px;border: solid 1px #ededed;border-width: 1px 0 0 ;position: relative;cursor: pointer;z-index: 5;background: #fff; margin-bottom:1px;
}
.m-accordion .m-header:hover, .m-accordion .hm-eader:focus {z-index: 2;background: #f0f0f0;}
.m-accordion .m-header a {color: #00a1e6; font:14px Arial;line-height: 40px;}
.m-accordion .m-inner-content {padding:0;background: #fff}
.m-accordion .m-inner-content ul{list-style:none; display:block; overflow:hidden}
.m-accordion .m-inner-content ul li{float:none; position:relative; margin:0 0 1px 0}
.m-accordion .m-inner-content ul li span{display:block; padding:12px; color:#ff8400; font:bold 14px Arial}
.m-accordion .m-inner-content ul li a{display:block; background:#f5f5f5!important; padding:10px 20px; color:#333;text-indent: 30px; font:bold 14px Arial; text-decoration:none}
/*.m-accordion .m-inner-content ul li a:hover{background:#f5f5f5; color:#333}
.m-accordion .m-inner-content ul li a:focus{background:#ff8400; color:white}*/
.m-accordion .m-inner-content ul li ul{display:block; overflow:hidden;}
.m-accordion .m-inner-content ul li li{float:none; border-bottom:1px solid #ccc}
.m-accordion .m-inner-content ul li li a{color:#333; font-size:13px; font-weight:normal; background:#fff !important;text-indent:40px;padding:10px 25px}
.m-accordion .m-inner-content ul li li:last-child{border:0}
.m-accordion h2 {margin: 0;	color: #000;text-shadow: none}
.m-indicators-images .m-header:after {content: "";display: block;position: absolute;top: 50%;right: 10px;margin-top: -11px;z-index: 4;width: 22px;height: 22px;}
.m-indicators-images .m-active .m-header:after {background-position: 0 -43px}
.m-arrows .m-active .m-header:after {top: 100%;	border: solid transparent;content: " ";	height: 0;width: 0;	position: absolute;pointer-events: none;	border-top-color: #f6f6f6;	border-width: 20px;	left: 50%;	margin-left: -20px;	z-index: 9}
.m-arrows .m-active .m-header:hover:after {	border-top-color: #e6e6e6}
.m-indicators-css .m-header:after {
	content: "▼";
	color: #999;
	text-shadow: #fff 0 1px 0;
	display: block;
	position: absolute;
	top: 50%;
	right: 15px;
	margin-top: -13px;
	z-index: 9;
	font-size: 18px;
	line-height: 22px;
	font-weight: 700
}
.m-indicators-css .m-active .m-header:after {content: "▲"; color:#00a1e6;}
.m-css-advanced .m-header:after {
	color: rgba(255,255,255,.9);
	font-weight: 400;
	text-align: center;
	text-shadow: none;
	background: #c5c5c5;
	background: -moz-linear-gradient(top, #c5c5c5 0, #acacac 100%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #c5c5c5), color-stop(100%, #acacac));
	background: -webkit-linear-gradient(top, #c5c5c5 0, #acacac 100%);
	background: -o-linear-gradient(top, #c5c5c5 0, #acacac 100%);
	background: -ms-linear-gradient(top, #c5c5c5 0, #acacac 100%);
	background: linear-gradient(to bottom, #c5c5c5 0, #acacac 100%);
	font-size: 22px;
	line-height: 16px;
	width: 20px;
	height: 20px;
	margin-top: -10px;
	-webkit-border-radius: 25px;
	-moz-border-radius: 25px;
	border-radius: 25px;
	-webkit-box-shadow: #fff 0 1px 0;
	-moz-box-shadow: #fff 0 1px 0;
	box-shadow: #fff 0 1px 0
}
.m-css-advanced .m-header:hover:after {
	color: #fff;
	background: #b4b4b4;
	background: -moz-linear-gradient(top, #b4b4b4 0, #a3a3a3 100%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #b4b4b4), color-stop(100%, #a3a3a3));
	background: -webkit-linear-gradient(top, #b4b4b4 0, #a3a3a3 100%);
	background: -o-linear-gradient(top, #b4b4b4 0, #a3a3a3 100%);
	background: -ms-linear-gradient(top, #b4b4b4 0, #a3a3a3 100%);
	background: linear-gradient(to bottom, #b4b4b4 0, #a3a3a3 100%)
}
.m-css-advanced .m-active .m-header:after {
	background: #8a8a8a;
	background: -moz-linear-gradient(top, #8a8a8a 0, #a3a3a3 100%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #8a8a8a), color-stop(100%, #a3a3a3));
	background: -webkit-linear-gradient(top, #8a8a8a 0, #a3a3a3 100%);
	background: -o-linear-gradient(top, #8a8a8a 0, #a3a3a3 100%);
	background: -ms-linear-gradient(top, #8a8a8a 0, #a3a3a3 100%);
	background: linear-gradient(to bottom, #8a8a8a 0, #a3a3a3 100%);
	-webkit-box-shadow: #fff 0 -1px 0;
	-moz-box-shadow: #fff 0 -1px 0;
	box-shadow: #fff 0 -1px 0
}
