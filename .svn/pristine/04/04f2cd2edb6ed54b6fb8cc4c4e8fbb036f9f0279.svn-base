.ac-container input:checked + label,.ac-container input:checked + label:hover{
	/*background:#ff8400 url(../../images/msite/arrow_up.png) no-repeat right 15px center;color: #fff;*/
	background-color:#ff8400;
	background-image:url(../../images/msite/arrow_up.png);
	background-repeat:no-repeat;
	background-position:right 15px center;
	color: #fff;
}
.ac-container label:hover:after,.ac-container input:checked + label:hover:after{content:'';position:absolute;width:24px;height:24px;
