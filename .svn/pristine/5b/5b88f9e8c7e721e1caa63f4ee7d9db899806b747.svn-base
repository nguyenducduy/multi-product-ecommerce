}


@media (min-width: 1024px) and (max-width: 1200px) {
    .showhide{display:none}
    ul.nav li ul li.specialclass{    width: 500px !important;}
    ul.nav li ul li{left:200px !important;}
    #tophead{ width: 980px; }
    
    #nav-main{width: 980px;}
    #banner{margin:0 auto; width: 980px;}
    
