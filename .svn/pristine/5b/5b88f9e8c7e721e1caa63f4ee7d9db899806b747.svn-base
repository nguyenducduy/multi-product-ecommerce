    .nav-news ul{float:left; width:980px; height:40px; margin:0 0 10px; }
    .adnewsnav{width:980px !important}
    .adnewssearch{width:980px !important}
    #wrap-news{width:980px !important;  margin:10px auto;}
    .news-left{float:left; width:660px !important; margin-right:20px; overflow:hidden}
    
    .marri{margin-right:0 !important;}
    .news980{display:block !important; width:660px; margin-bottom:0 !important}
        
    .news-right{float:left; width:300px; overflow:hidden;}
    .specialcol{display:none}
    .othernews{display:none}
    
    #adnews{width:980px !important;}
    .layeradd{left:115px !important;}
    .addtotal{ width:660px !important; margin-right: 10px !important;}
    .addcol{ width: 298px !important; margin: 35px 15px 20px !important;}
    .addlist{width:460px !important; margin:0 18px !important}
    .addleft{width:175px !important}
    
    .infotext{width:100% !important;}
    .infoimg{margin-left:65px;}
    .writepost{width:79% !important}

    .dropdown {height: 28px;left:-2px; border-left: 1px solid #ccc}
    .duy{width: 940px !important;}
    .searchstuff{width: 44% !important;}
	
	.colchoi{height:0 !important}
}

