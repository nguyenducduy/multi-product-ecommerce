          background-image: -webkit-linear-gradient(top, #ffffff, #f5f5f5);
          background-image: -o-linear-gradient(top, #ffffff, #f5f5f5);
          background-image: linear-gradient(to bottom, #ffffff, #f5f5f5);
         background-image: -moz-linear-gradient(top, #ffffff, #f5f5f5);
    }
    .products_name ul li{ float:left; color:#6f6d6d !important; padding:0 !important; width:auto !important; border:none !important; }
    .products_name ul li a{ font:normal 11px Arial !important;}
    .products_name ul li:hover{background:none !important}
    
    .navbarprod{ width:980px;}
    
    .navbarprod ul{float:left; width:980px; margin-left:2px;}
    
    .infoprod ul li a{font:normal 11px Arial;}
    
    .menufeature {width:980px; float:left}
    
    .hightlight{float:left;width:980px; }
    .hightlight .clum{float:left; width:30%;}
    
    .wrap-one{width:980px; clear:both}
    
    #keys{width: 960px; max-width: 96%; }
    #footer{width: 960px;}
    #copyright{ width: 960px; max-width: 96%; }
    
    .wi-inreg {width: 66%!important;}    
    #footer .colfoot-1{    float:left; width:229px; padding-bottom:8px;}
    #footer .colfoot-2{    float:left; width:20%; margin-left:20px;}
    #footer .colfoot-3{    float:left; width:25%; margin-left:20px;}
	#footer .colfoot-3 img {width: 45%;height: auto;}
    #footer .colfoot-4{    float:right; width:25%;margin-left:10px;}
    #footer .colfoot-4 .area{float:left; padding:2px; border-right:1px solid #ccc; font:normal 11px Arial; color:#666; text-align:center; margin-bottom:10px;}
    #footer .colfoot-4 .area span{font:bold 11px Arial; color:#333;}
	
    
    .rowsp{width:969px !important;}
    .lileft{margin:60px auto; width:200px}
	.lileft span{font: 14px/30px Arial;}
    .incomp{ width:180px !important;}
    .rownd{width:969px !important;}
    /*.contleft{width:622px;}*/
	.rowsp li{width: 220px;}
	.colcont{width: 224px;}
    .contleft_3sp{width:642px}
    .contleft_4sp{width:664px;}
    .contleft_5sp{width:980px;}
    .contleft_6sp{width:980px;}
    .col_sp{ width:176px !important}
    
    .contrigh{ width:345px; margin:0 auto !important; overflow:hidden; background:white}
    .cori-3sp{width:300px;}
    .cori-4sp{width:300px}
    
    .similar{float:left; width:100% ; margin:10px}
    .similar li{float:none; list-style:none; margin:10px auto; padding:8px; }
    .wi_li{width:280px;}
    .wi-3sp{width:20% !important}
    .wi-5sp{width:172px !important}
    .wi-6sp{width:172px !important}
    
    .wi_li_3sp{width:197px; font:normal 11px/20px Arial !important;}
    .wi_li_4sp{width:150px;font:normal 11px/20px Arial !important;}
    
    /*News*/
