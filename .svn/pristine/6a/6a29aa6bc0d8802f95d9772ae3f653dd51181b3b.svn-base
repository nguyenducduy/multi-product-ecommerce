.like-face{ float:right; margin-top:5px; position:absolute; right:395px; z-index:2;}

.btn-muangay2 { margin-top:17px; float:right; margin-right:10px; width:110px;}
.btn-mua2 {
	cursor: pointer;	padding:12px 5px;	width:100px;	font:bold 16px Arial;	color:#ea0000;	text-transform:uppercase;
	text-align:center;	text-decoration: none;	text-shadow:1px 1px 1px rgba(0,113,186,.3);
	
	border-radius: 4px; /* for Mozilla browsers */
	-webkit-border-radius: 4px; /* for webkit browsers (Chrome, Safari) */
	-o-border-radius:4px;
	-khtml-border:4px; 
	border-radius: 4px; /* for Opera and others */
	background:#fff200;}
	
.btn-mua2:hover {
	color:#ea0000;
	background:#ffb000;
	text-shadow:1px 2px 1px rgba(0,0,0,.3);
	
	
}
.productimagegr{
	width:210px; height:225px;position: relative;
}
.productimagegr img{
	position: absolute;
	top: 0;
	left: 0;
	right: 0;
	bottom: 0;
	margin: auto;
	max-width: 210px;
	max-height: 225px;
}
.images-giare{
	position: relative;

}
.images-giare img{
	width: 44px;
	height: 45px;
}
.percent-giare{
	position: absolute;
	top: 20%;
	left: 60%;
	color: #fff;
	z-index: 1;
	-webkit-transform: rotate(45deg);
	-moz-transform: rotate(45deg);
	-ms-transform: rotate(45deg);
	-o-transform: rotate(45deg);
	filter: progid:DXImageTransform.Microsoft.BasicImage(rotation=2);
	font-weight: bold;
	font-family: arial;
	font-size: 12px;
}