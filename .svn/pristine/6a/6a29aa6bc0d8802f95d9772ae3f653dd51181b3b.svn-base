.link-detail a:hover{color:#00a1e5; text-decoration:none;}

.prices-red{ margin-top:5px; float:right; margin-right:5px;  background:url(../../images/giare/giarenew/bg-price.png) no-repeat;  height:38px; font:bold 33px Arial; color:#e90000; padding:0 22px;  }
.price-bl {font:normal 14px Arial; color:#18110b; text-decoration:line-through; margin-left:10px;}
