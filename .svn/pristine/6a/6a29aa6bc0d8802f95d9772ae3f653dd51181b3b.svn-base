/* CSS Document */
*{margin:0; padding:0; outline:none}
img{border:0; vertical-align:middle}
.clear{clear:both}

#wrapper-promotion-ver2 {width: 1000px; margin:0 auto;font:normal 12px Arial;overflow:hidden; }

#pro-top-ver2{ margin:auto; width:1000px; position:fixed; z-index:9; top:0; border-top: solid 2px #00a1e4; background:#fff; }
.pro-logo{margin-left:5px; float:left; width:220px; margin-top:4px;}

#pro-header { margin:auto; height:80px; }
.top-nav{ margin:auto; height:22px; padding:4px 7px;  border-bottom:solid 1px #e1e1e1; float:right; width:680px;}

.topleft ul{ margin-right:10px;  list-style:none; margin-top:2px;}
.topleft li{ float:left; color:#ccc; font:bold 11px Arial; color:#444; position:relative; padding-right:5px; padding-top:2px;}
.number a{font:bold 11px Arial; color:#df1835;text-decoration:none; }
.number a:hover{ color:#0072c1; text-decoration:none;
	
}
.number{
	float: none !important;
	width: auto !important;
	position: relative;
	margin-top: 0 !important;
	border: none !important;
}
.top-right-hot{ float:right; margin-right:10px;  height: 22px !important;
    line-height: 22px !important;
    overflow: hidden;
	background:none;}
.top-right-hot ul li a{ font:bold 11px Arial;color:#0072c1; text-decoration:none;}
.top-right-hot ul li a:hover{ color:#df1835; text-decoration:none;}

.ticker2 {
	float:right;display:block;margin-right:10px;list-style: none outside none;overflow: hidden; margin-top:-3px;
	width: 390px;
	text-align: right;
}
.ticker2 li {height: 22px;}


.nav-main{margin:auto; width:680px}
.nav-main ul{ float:left; list-style:none; margin-left:60px;}
.nav-main ul li{  padding:14px 10px;  height:18px; font:bold 12px Arial; color:#555; float:left}
.nav-main ul li a{  padding:14px 10px; font:bold 12px Arial; color:#555; text-decoration:none; text-transform:uppercase;}
.nav-main ul li a:hover{color:#00a1e4; text-decoration:none;  border-bottom:2px solid #00a1e4}	
/*.nav-main ul li .select{font:bold 12px Arial;color:#00a1e4; text-decoration:none; padding:14px 10px; border-bottom:2px solid #00a1e4}	*/
.nav-main .current a {
    font:bold 12px Arial;color:#00a1e4; text-decoration:none; padding:14px 10px; border-bottom:2px solid #00a1e4
}

.banner-multpro-v2{ width:1000px; margin-top:80px}

.content-multpro-v2{ width:1000px; margin:0 auto; margin-top:10px;}
.filter-multpro-v2{ background:#d9f4ff; width:100%; margin:0 auto; height:40px;}
.filter-title{ float:right;color:#444;}
.filter-multpro-v2 ul{display:inline; list-style:none; width:98%; margin:0 auto; }
.filter-multpro-v2 ul li{float:left; padding-left:20px; font:normal 13px Arial; color:#444; margin-top:10px;}

.box-product-ver2{ margin:0 auto; margin-top:20px; overflow:hidden; background:#f5f5f5; padding:20px 0 0 0;}
.pro-ver2 ul{ with:98%; display: inline; list-style:none; margin:0 auto; }
.pro-ver2 ul li{ float:left; border-radius:15px 0 15px 0; border:solid 1px  #52cbfc; background:#fff; width:220px; margin-left:22px; margin-bottom:20px; position:relative; }
.pro-ver2 ul li .special-label-v2{ height: 45px; position: absolute;right:9px;top:0;width: 45px;}
.pro-ver2 ul li img{ padding:0 10px;}
.pro-name { border-bottom:solid 1px #e2e2e2; text-transform:uppercase; height:40px; width:180px; margin:5px auto 0; }
.pro-name a{ font:bold 12px/18px Arial; text-decoration:none; color:#666; text-transform:uppercase;}
.pro-name a:hover{text-decoration:none; color:#0089cf;}
.pro-bottom{ margin:0 auto; border-top:solid 1px #e2e2e2; width:200px; height:50px; }

.price-button{ float:left; background:#bfbfbf; margin-top:16px; font:bold 14px Arial; color:#fff; text-transform:uppercase;}
.price-button a{background:#bfbfbf; margin-top:10px; font:bold 14px Arial; color:#fff; text-decoration:none;padding:7px 24px;}
.price-button a:hover{background:#0089d0; margin-top:10px; font:bold 14px Arial; color:#fff; padding:7px 24px;text-decoration:none;}

.price-dis{font:bold 16px Arial; color:#d90000; float:right; margin-top:6px;}
.price-main{font:normal 13px Arial; color:#777; text-decoration:line-through; float:right;}

select  {display: inline-block; padding: 4px 6px; font-size: 12px; margin-top:-5px; color: #444;  -webkit-border-radius: 3px;-moz-border-radius: 3px;border-radius: 3px;}

select,input[type="file"] {  height: 30px;
  /* In IE7, the height of the select element cannot be changed by height, only font-size */

  *margin-top: 4px;
  /* For IE7, add top margin to align select with labels */

  line-height: 30px;
}

select { width: 210px;  background-color: #ffffff;  border: 1px solid #b7b7b7;}
.radio {padding-left:20px;}

.box-priciple-ver2{ width:100%; margin-top:20px; overflow:hidden; }
.priciple-v2-ctent{ width:90%; margin-top:10px; margin:auto; font:normal 13px/25px Arial; color:#444; margin-left:80PX}
.priciple-v2-title{font:normal 20px Arial; color:#0089cf; text-transform:uppercase; padding:15px 10px;}

.box-sale-ver2{ width:99%; margin:0 auto; padding:15px 5px; margin-top:30px; overflow:hidden; border-top:solid 1px #ccc;}
.box-sale-ver2 img{margin:10px auto 5px;}
.box-sale-ver2 ul{ width:98%; padding:0 12px;list-style:none; margin-top:5px;  }
.box-sale-ver2 ul li { width:310px; float:left; padding-left:10px;}
.sale-v2-title{ font:normal 20px Arial; color:#0089cf; text-transform:uppercase; margin-top:10px;}
.sale-v2-name a{ font:bold 13px Arial; color:#0096f8; margin-top:10px; text-decoration:none; }
.sale-v2-name a:hover{ color:#ff0000; text-decoration:none;}
.sale-v2-text{font:normal 12px/20px Arial;float:left; color:#757575;padding:5px 2px;}

.box-support-ver2{ margin:0 auto; width:99%; overflow:hidden;}
