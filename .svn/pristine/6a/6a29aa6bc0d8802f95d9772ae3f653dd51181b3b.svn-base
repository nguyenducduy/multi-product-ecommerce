    width: 240px; color:#555;}
	
.search-bill-v2 input:focus  {
    border-color: rgba(82, 168, 236, 0.8);
    box-shadow: 0 1px 1px rgba(0, 0, 0, 0.075) inset, 0 0 8px rgba(82, 168, 236, 0.6);
}
.btn-search {background: none repeat scroll 0 0 #00A1E8;border-radius: 3px 3px 3px 3px;
   
	margin-left:5px;
    padding: 10px 20px;
    text-align: center;
}


.right-support-v2{ float:right; width:40%;margin-right:10px;margin-top:10px; border-bottom:solid 1px #888; background:#f9f9f9;padding:20px 0; }
.r-support-v2-title{font:normal 20px Arial; color:#0089cf; text-transform:uppercase; padding:0px 5px;}
.date-v2{ font:normal 13px Arial; color:#666; text-transform:none; padding-top:10px; float:left; margin-left:80px; padding-bottom:20px}
.hotline-support-v2{ float:left; margin-left:5px; margin-top:10px; font:bold 20px Arial; color:#ff0000;}

.box-footer-ver2{ width:100%; margin:0 auto;background:#222; position:relative; margin-top:30px; height:130px;}
.footer-v2-ctent{ margin-left:12px;}
.footer-v2-ctent ul{ width:980px;  list-style:none; display:inline;}
.footer-v2-ctent ul li{ width:325px; float:left;  text-align: center;}
.f-ter-col1 { background:#00aeed;}
.f-ter-col2 { background:#0384ce;}
.f-ter-col3 { background:#3156a3;}

.title-f-v2{ font:bold 13px/20px Arial; color:#fff; padding:8px 30px; text-transform:uppercase;}
.text-f-v2{font:normal 12px/20px Arial; color:#fff; padding:10px 14px;}

.tooltip-gift2 {
    font:bold 12px/20px Arial;
	color:#444;
    background:#fff;
    display:none; /*--Hides by default--*/
   	
	margin-right:10px;
	float:right;
	padding:5px 10px;
	z-index:1000;
    border-radius: 4px;
	border:solid 1px #999;
	
}


[class^="icon-"],
[class*=" icon-"] {
  display: inline-block;
  width: 30px;
  height: 30px;
  margin-top: 1px;
  line-height: 30px;
  vertical-align:middle;
  background-image: url(../../images/giare/giarenew/icon-page-promulti2.png) ;
  background-repeat: no-repeat;
}
