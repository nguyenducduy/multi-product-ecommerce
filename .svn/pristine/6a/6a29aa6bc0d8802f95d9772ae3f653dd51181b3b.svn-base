.icon-cart-v2 { background-position:-20px 0; width:20px; height:20px; margin-right:5px; margin-top:-4px; }
.icon-arr-v2 { background-position:-40px 0; width:30px; height:20px; margin-right:5px; float:left; margin-top:-1px; }
.icon-princ-v2 { background-position:0 -20px; width:60px; height:40px; margin-right:10px; float:left; margin-top:-5px;}
.icon-sale-v2 { background-position:-60px -20px; width:60px; height:40px; margin-right:10px; float:left; margin-top:-5px;}
.icon-check-v2 { background-position:-120px -20px; width:60px; height:40px; margin-right:10px; float:left; margin-top:-5px;}
.icon-phone-v2 { background-position:-180px -20px; width:60px; height:40px; margin-right:10px; float:left; margin-top:-5px;}
.icon-search-v2 { background-position:-70px 0; width:20px; height:20px; }
.icon-user-v2 { background-position:-90px 0; width:20px; height:20px; margin-top:-3px; margin-right:5px; }
.icon-thumb-v2 { background-position:0px -60px; width:80px; height:80px; margin:15px 10px 0 15px; float:left }
.icon-delivery-v2{ background-position:-80px -60px; width:80px; height:80px; margin:15px 10px 0 15px; float:left }
