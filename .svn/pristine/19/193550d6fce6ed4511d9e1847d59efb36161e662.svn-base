
.pre_center{float:left; overflow:hidden; background:#fff; margin:0 15px; width:550px}
.des_pre{display:block; overflow:hidden; background:#6aaa16; margin:5px; box-shadow:0 2px 8px rgba(0,0,0,0.3);}

.des_pre h3{font:bold 16px Arial; color:white; margin:6px 0; display:block; text-transform:uppercase; border-bottom:1px solid #97c45c; text-shadow:0 1px 2px rgba(0,0,0,0.3);}
.des_pre h3 span{display:block; background:#86c342; padding:8px;}
.des_pre h3 strong{font-weight:bold; display:block; padding:3px 8px; font-size:20px;}
.area_pre{display:block; color:white; font:14px Arial; margin:0 10px; overflow:hidden;margin-bottom: 10px}
.pre_price{display:block; margin-bottom:10px; overflow:hidden}
.pre_price strong{font-weight:bold; float:left; margin:14px 0 0}
.pre_price .pricepre{float:left; margin-left:10px; background:#6aaa16; border:1px dashed #97c45c; border-radius:4px 4px 4px 4px; padding:1px}
.pre_price .pricepre span{background:white; color:red; font:bold 30px Arial; padding:3px 12px; border-radius:4px 4px 4px 4px; display:block}
.pre_price .pricepre span sub{vertical-align:text-bottom; font-size:20px; font-weight:bold}

.pre-statistic{display:block; overflow:hidden}
.pre-statistic span{padding:2px 0}
.pre-statistic .fbcm{display:block; padding:6px 0}
.pre-statistic .cellsmall{float:left; font-weight:bold; overflow:hidden;}
.pre-statistic .cellcomm{float:left; font-weight:bold; cursor:pointer;margin-top: 2px;}
.pre-statistic .cellcomm a{display:block; text-decoration:none; color:white}
.pre-statistic .cellcomm a:hover{text-decoration:underline}

.bar_pre_center{display:block; margin:0px 10px 0; overflow:hidden; background:#f0f0f0; border-top:3px solid #fc3449; border-radius:6px 6px 0 0; padding:10px}
.bar_pre_center .titinfo{display:block; overflow:hidden; text-transform:uppercase; color:#333; font:bold 15px Arial;}

.icongiftpre{background: url(../../images/msite/bg_pre_order.png) no-repeat; background-position:0 0; width:20px; height:20px; float:left; margin:-2px 3px 0 0;}
.iconlistpre{background: url(../../images/msite/bg_pre_order.png) no-repeat; background-position:-20px 0; width:20px; height:20px; float:left; margin-right:5px}
.iconcheckpre{background: url(../../images/msite/bg_pre_order.png) no-repeat; background-position:-40px 0; width:20px; height:20px; float:left; margin-right:5px}
.iconcartpre{background: url(../../images/msite/bg_pre_order.png) no-repeat; background-position:-60px 0; width:30px; height:25px; float:left; margin-right:5px}

.giftpromo_pre{display:block; overflow:hidden; background:white; padding:10px; border:1px solid #bcc0b7; border-bottom:3px solid #70b613}
.infogift_pre{display:block; overflow:hidden; margin-right:10px; line-height:20px}
.infogift_pre span{font-weight:bold; display:block; padding:5px 0; font-size:14px; color:#333}

.area_time_pre{display:block; margin-top:10px; background:#fff; padding:5px 10px}
.area_time_pre .date_pre{display:block; padding:0 0 6px 0; color:#333; font:14px Arial; overflow:hidden}
.area_time_pre .date_pre strong{font-weight:bold; color:red; font-size:18px}
.pre_countdown{display:block; margin-bottom:20px; overflow:hidden}
.pre_countdown span{float:left; margin-top:16px; margin-right:8px}
.pre_countdown .timedown{width: 100%; margin:10px 0 0; height: 32px;font-weight: bold;}
.btn-pre {display:block; overflow:hidden; margin:auto; width:235px; border: 1px solid #BCBCBC;}
.btn-pre a {text-align:center;
    background:#FF3C1F;
    border: 1px solid #FFFFFF;
    color: #FFFFFF;
    display: block;
    font:bold 22px Arial;
    outline: medium none;
    padding: 10px 8px 8px 10px;
    text-decoration: none;
    text-transform:uppercase;
}
.btn-pre a:hover {
    background: none repeat scroll 0 0 #EC3318;
}

#lablecounterprepay {
	width: 100%;
	font: 12px arial;
	color: #555;
	overflow: hidden;
	text-align: center;
	margin-left: 48px;
}
#lablecounterprepay span {
margin: 0px 14px;
}
